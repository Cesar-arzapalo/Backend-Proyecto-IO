set default
set echoin 1
MODEL:
SETS:
  	Patron/1..4/:CnP, OP, CP;

ENDSETS

DATA:

	!Presupuesto;
	P=@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf');
	!Demanda insatizfecha de estudiantes;
	DIE=@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf');
	!Demanda total de estudiantes;
	DTE=@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf');

	!Ocupantes por patron;
	OP=@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf');
	!Costo por patron;	
	CP=@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf');

	!Cantidad del patron de tipo j;
	@OLE('C:\Users\Minamura\Documents\Investigacion Operativa I\Proyecto\lingo\patronesEjecutable.ltf')=CnP;
	
